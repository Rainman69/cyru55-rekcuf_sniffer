<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="40dp"
    android:background="@color/white"
    tools:context=".one">

    <ToggleButton
        android:id="@+id/netstat"
        style="@style/toggleButton"
        android:layout_width="128px"
        android:layout_height="128px"
        android:background="@drawable/net_toggle_bg"
        android:enabled="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="DuplicateSpeakableTextCheck"
        />

    <LinearLayout
        android:id="@+id/row2"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/bgradius"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingVertical="0dp"
        android:paddingLeft="8dp"
        android:paddingRight="8dp"
        app:layout_constraintTop_toBottomOf="@+id/netstat"
        >

        <EditText
            android:id="@+id/inp1"
            android:layout_width="32dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:ems="3"
            android:gravity="center_horizontal"
            android:inputType="number"
            android:maxLength="5"
            android:importantForAutofill="no"
            android:hint="0"
            android:clickable="false"
            android:cursorVisible="false"
            android:focusable="false"
            android:textColor="@android:color/darker_gray"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Domain"
            android:textSize="16sp"
            android:textStyle="bold"
            />

        <EditText
            android:id="@+id/inp2"
            android:layout_width="32dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:ems="3"
            android:gravity="center_horizontal"
            android:text="0"
            android:importantForAutofill="no"
            android:inputType="number"
            android:maxLength="5"
            android:clickable="false"
            android:cursorVisible="false"
            android:focusable="false"
            android:textColor="@android:color/darker_gray"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Sent"
            android:textSize="16sp"
            android:textStyle="bold"
            />

        <EditText
            android:id="@+id/inp3"
            android:layout_width="32dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:ems="3"
            android:gravity="center_horizontal"
            android:inputType="number"
            android:maxLength="6"
            android:hint="0"
            android:importantForAutofill="no"
            android:clickable="false"
            android:cursorVisible="false"
            android:focusable="false"
            android:textColor="@android:color/darker_gray"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Total"
            android:textSize="16sp"
            android:textStyle="bold"
            />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/row3"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/bgradius"
        android:orientation="horizontal"
        android:paddingLeft="8dp"
        android:paddingRight="8dp"
        app:layout_constraintTop_toBottomOf="@+id/row2">

        <EditText
            android:id="@+id/inp4"
            android:layout_width="32dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:ems="2"
            android:gravity="center_horizontal"
            android:inputType="number"
            android:maxLength="2"
            android:text="4"
            android:importantForAutofill="no"
            android:hint="conc"
            />

        <TextView
            android:id="@+id/text_r2_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Conc"
            android:textSize="16sp"
            android:textStyle="bold"
            />

        <EditText
            android:id="@+id/inp5"
            android:layout_width="32dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:ems="2"
            android:gravity="center_horizontal"
            android:inputType="number"
            android:maxLength="2"
            android:text="5"
            android:importantForAutofill="no"
            android:hint="5"
            />

        <TextView
            android:id="@+id/text_r2_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="TimeOut"
            android:textSize="16sp"
            android:textStyle="bold"
            />

        <CheckBox
            android:id="@+id/checkbox1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="true"
            android:scaleX="1.4"
            android:scaleY="1.4"
            tools:ignore="TouchTargetSizeCheck"
            />

        <TextView
            android:id="@+id/text_r2_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Notif"
            android:textSize="16sp"
            android:textStyle="bold"
            />

    </LinearLayout>

    <ScrollView
        android:id="@+id/logger_parent"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:paddingRight="16dp"
        android:paddingLeft="16dp"
        app:layout_constraintBottom_toTopOf="@+id/row7"
        app:layout_constraintTop_toBottomOf="@+id/row3"
        android:contentDescription="logs"
        >

        <LinearLayout
            android:id="@+id/logger"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:scrollbars="vertical"
            />

    </ScrollView>

    <LinearLayout
        android:id="@+id/row7"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/bgradius"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        >

            <TextView
                android:id="@+id/text_r7_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:text="BgService"
                android:textSize="16sp"
                android:textStyle="bold"
                />
            <ToggleButton
                android:id="@+id/togglev1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/togglev1"
                android:enabled="false"
                android:text="ToggleButton"
                android:textOn="@cyru55"
                android:textAllCaps="false"
                />

        <com.google.android.material.switchmaterial.SwitchMaterial
            android:id="@+id/switch1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:gravity="center"
            android:layout_gravity="center"
            android:paddingStart="-40dp"
            android:paddingEnd="40dp"
            android:paddingRight="40dp"
            android:paddingLeft="-40dp"
            android:scaleX="1.4"
            android:scaleY="1.4"
            />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
